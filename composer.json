{
    "name": "modernmcguire/headstart",
    "description": "A CMS built on Laravel",
    "keywords": [
        "modernmcguire",
        "headstart",
        "laravel",
        "cms",
        "admin panel"
    ],
    "homepage": "https://github.com/modernmcguire/headstart",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tommy McGuire",
            "email": "tommy@modernmcguire.com",
            "role": "Developer"
        },
        {
            "name": "Ben Miller",
            "email": "ben@modernmcguire.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "bensampo/laravel-enum": "^3.1",
        "illuminate/support": "^8.0",
        "spatie/laravel-medialibrary": "^9.1",
        "spatie/laravel-sluggable": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ModernMcGuire\\Headstart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModernMcGuire\\Headstart\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModernMcGuire\\Headstart\\HeadstartServiceProvider"
            ],
            "aliases": {
                "Headstart": "ModernMcGuire\\Headstart\\HeadstartFacade"
            }
        }
    }
}
